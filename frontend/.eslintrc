{
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
    "ecmaVersion": 8
  },
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		"no-console":"error",
		"semi": "error"
	}
}